################################################################################
################################################################################
# Custom Build MenuItem Configuration
# 
# Limit to 80 characters 【注：这部分后边根据视需求和实现难度，可单独放到一个文件中】
# Product Demo 对应的CutomBuild菜单显示
# DisplayName ; 宏定义（编译可能用到） ; 是否选择; 对应的编译模块id(格式id1-id2-..）；不同产品形态是否需要编译多次
# 
################################################################################
#是否显示Custom Build菜单，1：显示；0：不显示
CUSTOM_BUILD_MENU_SUPPORT = 1
#交付件是否生成：
#第一列：菜单显示的名称，带OPTION的表明包含子菜单，_SEPERATOR_表示该行为分割线
#第二列，对于子菜单，表示*.cfg中的宏名，对于父菜单，表示对应子菜单的后缀，例如：HB_CUSTOM_BUILD_TARGET_DELIV_OPTION_SDK是HB_CUSTOM_BUILD_TARGET_DELIV_OPTION的子菜单
#第三列：对子菜单，表示菜单是否被选择，对父菜单，暂无含义。
#第四列：仅对子菜单有效，表示编译模块id，跟以上表格对应
#第五列：别名（标示符）
#第六列：BOOL，是否作用应用层编译选项，用其别名修改hb.cfg中的HB_APP_COMPILE_LIST

HB_CUSTOM_BUILD_TARGET_DELIV_OPTION = SDK						;SDK 							;NO			;NONE		;SDK				;NO
#SDK生成编译方式的选择，MAKE_SDK表示只make，COMPILE_SDK表示make后compile							
HB_CUSTOM_BUILD_TARGET_DELIV_OPTION_SDK = MAKE_SDK				;HB_SDK_MAKE_ENABLE				;NO			;NONE		;MAKE_SDK			;NO
HB_CUSTOM_BUILD_TARGET_DELIV_OPTION_SDK = COMPILE_SDK			;HB_SDK_COMPILE_ENABLE			;NO			;NONE		;COMPILE_SDK		;NO

HB_CUSTOM_BUILD_TARGET_DELIV = HSO_PC	    					;HB_HSO_PC_COMPILE_ENABLE		;NO			;14			;HSO_PC				;NO
HB_CUSTOM_BUILD_TARGET_DELIV = BOOT_LIB	    					;HB_BOOT_COMPILE_ENABLE			;YES		;1			;BOOT_LIB			;NO
HB_CUSTOM_BUILD_TARGET_DELIV = MRS		    					;HB_APP_COMPILE_MRS_ENABLE		;YES		;10			;MRS				;YES
HB_CUSTOM_BUILD_TARGET_DELIV = IPMRS	    					;HB_APP_COMPILE_IPMRS_ENABLE	;NO			;11			;IPMRS				;YES
HB_CUSTOM_BUILD_TARGET_DELIV = EQT		    					;HB_APP_COMPILE_EQT_ENABLE		;NO			;12			;EQT				;YES
HB_CUSTOM_BUILD_TARGET_DELIV = HANDHELD	    					;HB_HAND_HELD_COMPILE_ENABLE	;NO			;NONE		;HANDHELD			;NO
HB_CUSTOM_BUILD_TARGET_DELIV = _SEPERATOR_    					; 								; 			; 			; 					; 
#模块编译行为（暂支持单选，选且必须选择一项，前边不可勾选）
HB_CUSTOM_BUILD_TARGET_OPTION = OS								;OS 							;NO			;NONE		;OS					;NO
HB_CUSTOM_BUILD_TARGET_OPTION_OS = OS_COMPILE_USE_SRC			;HB_LITEOS_COMPILE_ENABLE		;NO			;NONE		;OS_COMPILE_USE_SRC	;NO
################################################################################
################################################################################



